    <div class="swiper-wrapper ml-0 md:ml-0">
        <div class="swiper-slide p-3 md:p-0">
            <div
              class="grid flex-shrink-0 p-5 rounded-[10px] let-shadow  w-full md:w-64 2xl:w-72 h-64 2xl:h-72 mb-1"
            >
               <Image loading="eager" 
                                    class="justify-self-end mb-10"
                                    src={fortune}
                                    alt="Fortune 500"
                                />
              <p
                class="text-[#31393C] text-[4rem] leading-[4.75rem] font-bold"
              >
                5000+
              </p>
              <p class="text-[#31393C] text-base">
                Companies are using ERPNext
              </p>
            </div>
          </div>

          <div class="swiper-slide p-3 md:p-0">
            <div
              class="grid flex-shrink-0 p-5 rounded-[10px] let-shadow  w-full md:w-64 2xl:w-72 h-64 2xl:h-72 mb-1"
            >
               <Image loading="eager" 
                                    class="justify-self-end mb-10"
                                    src={diverse_industries}
                                    alt="Fortune 500"
                                />
              <p
                class="text-[#31393C] text-[4rem] leading-[4.75rem] font-bold"
              >
              25+
              </p>
              <p class="text-[#31393C] text-base">
                Serving 25+ Diverse Industries
              </p>
            </div>
          </div>

          <div class="swiper-slide p-3 md:p-0">
            <div
              class="grid flex-shrink-0 p-5 rounded-[10px] let-shadow  w-full md:w-64 2xl:w-72 h-64 2xl:h-72 mb-1"
            >
               <Image loading="eager" 
                                    class="justify-self-end mb-10"
                                    src={integration_tool}
                                    alt="Fortune 500"
                                />
              <p
                class="text-[#31393C] text-[4rem] leading-[4.75rem] font-bold"
              >
              50+
              </p>
              <p class="text-[#31393C] text-base">
                Support Integrations with over 50+ tools
              </p>
            </div>
          </div>

          <div class="swiper-slide p-3 md:p-0">
            <div
              class="grid flex-shrink-0 p-5 rounded-[10px] let-shadow  w-full md:w-64 2xl:w-72 h-64 2xl:h-72 mb-1"
            >
               <Image loading="eager" 
                                    class="justify-self-end mb-10"
                                    src={support_business}
                                    alt="Fortune 500"
                                />
              <p
                class="text-[#31393C] text-[4rem] leading-[4.75rem] font-bold"
