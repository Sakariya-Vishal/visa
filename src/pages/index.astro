			var scrollAmount = 0;
			const windowWidth = window.innerWidth;

			// if window screen medium
			if (windowWidth < 1024) {
				const elementsWithAriaCurrent = document.querySelectorAll(
					'[aria-current="true"]'
				);

				elementsWithAriaCurrent.forEach((element) => {
					console.log("element", element.innerHTML);
					if (element.innerHTML === "Manufacturing") {
						// var scroll = -containerElement.offsetWidth;
						// scrollAmount = -containerElement.offsetWidth
						scrollAmount = -10000;
						console.log("asdf", scrollAmount);
					} else {
						scrollAmount = 245;
					}
				});
			}
			// if window screen large
			else {
				const elementsWithAriaCurrent = document.querySelectorAll(
					'[aria-current="true"]'
				);

				elementsWithAriaCurrent.forEach((element) => {
					console.log("element", element.innerHTML);
					if (element.innerHTML === "Manufacturing") {
						var scroll = -containerElement.offsetWidth;
						scrollAmount = scroll - 40;
					} else {
						scrollAmount = 250;
					}
				});
			}

			// Perform horizontal scroll using the scrollBy method
			containerElement.scrollBy({
				top: 0,
				left: scrollAmount,
				behavior: "smooth",
			});
		});

	// scroll pre.
	document.getElementById("preScrollButton").addEventListener("click", () => {
		// Get the container element by its ID
		const containerElement = document.getElementById("scrollContainer");

		// Calculate the scroll amount
		// const scrollAmount = -containerElement.offsetWidth;

		var scrollAmount = 0;
		const windowWidth = window.innerWidth;

		if (windowWidth < 1024) {
			const elementsWithAriaCurrent = document.querySelectorAll(
				'[aria-current="true"]'
			);

			elementsWithAriaCurrent.forEach((element) => {
				console.log("element", element.innerHTML);
				if (element.innerHTML === "Ayurveda") {
					var scroll = containerElement.offsetWidth;
					scrollAmount = 10000;
				} else {
					scrollAmount = -250;
				}
			});
		} else {
			const elementsWithAriaCurrent = document.querySelectorAll(
				'[aria-current="true"]'
			);

			elementsWithAriaCurrent.forEach((element) => {
				console.log("element", element.innerHTML);
				if (element.innerHTML === "Ayurveda") {
					var scroll = containerElement.offsetWidth;
					scrollAmount = scroll + 60;
				} else {
					scrollAmount = -250;
				}
			});
		}

		// Perform horizontal scroll using the scrollBy method
		containerElement.scrollBy({
			top: 0,
			left: scrollAmount,
			behavior: "smooth",
		});
	});

